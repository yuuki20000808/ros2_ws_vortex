#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'four-team-ex-cnp','console_scripts','ex_controller_005'
__requires__ = 'four-team-ex-cnp'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('four-team-ex-cnp', 'console_scripts', 'ex_controller_005')()
    )
