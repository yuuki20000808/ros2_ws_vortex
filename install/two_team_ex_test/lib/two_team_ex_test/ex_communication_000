#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'two-team-ex-test','console_scripts','ex_communication_000'
__requires__ = 'two-team-ex-test'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('two-team-ex-test', 'console_scripts', 'ex_communication_000')()
    )
